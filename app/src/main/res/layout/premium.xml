<?xml version="1.0" encoding="utf-8"?><RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"    xmlns:app="http://schemas.android.com/apk/res-auto"    xmlns:tools="http://schemas.android.com/tools"    android:layout_width="match_parent"    android:layout_height="match_parent"    android:background="@color/main"    android:scrollbars="none">    <ScrollView        android:layout_width="match_parent"        android:layout_height="match_parent">        <RelativeLayout            android:layout_width="match_parent"            android:layout_height="match_parent">            <ImageView                android:layout_width="match_parent"                android:layout_height="wrap_content"                android:layout_alignParentBottom="true"                android:src="@color/new_main_c" />            <RelativeLayout                android:layout_width="match_parent"                android:layout_height="match_parent"                android:orientation="vertical">                <LinearLayout                    android:id="@+id/r1"                    android:layout_width="match_parent"                    android:layout_height="wrap_content"                    android:orientation="vertical">                    <androidx.appcompat.widget.Toolbar                        android:id="@+id/toolbar_disconnected"                        android:layout_width="match_parent"                        android:layout_height="?android:attr/actionBarSize"                        android:fitsSystemWindows="true"                        android:showAsAction="always"                        app:contentInsetLeft="0dp"                        app:contentInsetStart="0dp"                        app:layout_collapseMode="pin"                        app:layout_scrollFlags="scroll|enterAlways"                        app:theme="@style/ThemeOverlay.AppCompat.Dark.ActionBar">                        <RelativeLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:layout_marginStart="8dp"                            android:layout_marginLeft="8dp"                            android:layout_marginTop="20dp"                            android:layout_marginEnd="8dp"                            android:layout_marginRight="8dp"                            android:orientation="horizontal">                            <ImageView                                android:id="@+id/toolbar_back_button"                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_gravity="center"                                android:layout_marginStart="15dp"                                android:layout_marginTop="5dp"                                android:layout_marginRight="16dp"                                android:background="@drawable/ic_back"                                android:backgroundTint="@color/text_color"                                android:textColor="@color/text_color"                                android:textSize="16sp" />                            <TextView                                android:layout_width="match_parent"                                android:layout_height="wrap_content"                                android:layout_centerInParent="true"                                android:layout_gravity="center"                                android:layout_marginStart="100dp"                                android:fontFamily="@font/lexend"                                android:gravity="center"                                android:text="Premium Plan"                                android:textColor="@color/color_white"                                android:textSize="20sp" />                        </RelativeLayout>                    </androidx.appcompat.widget.Toolbar>                </LinearLayout>                <ImageView                    android:layout_width="match_parent"                    android:layout_height="match_parent"                    android:layout_below="@id/r1"                    android:layout_marginTop="20dp"                    android:adjustViewBounds="true"                    android:scaleType="centerCrop"                    android:src="@drawable/new_new_home_bg" />                <LinearLayout                    android:layout_width="match_parent"                    android:layout_height="match_parent"                    android:layout_below="@id/r1"                    android:orientation="vertical">                    <RelativeLayout                        android:id="@+id/upgradeToPro"                        android:layout_width="match_parent"                        android:layout_height="wrap_content">                        <ImageView                            android:layout_width="200dp"                            android:layout_height="200dp"                            android:layout_centerInParent="true"                            android:layout_gravity="center"                            android:layout_marginTop="10dp"                            android:src="@drawable/ic_new_paid_icon_serverlist" />                    </RelativeLayout>                    <LinearLayout                        android:layout_width="match_parent"                        android:layout_height="wrap_content"                        android:orientation="vertical">                        <TextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_gravity="center_horizontal"                            android:fontFamily="@font/lexend"                            android:text="Upgrade to PRO"                            android:textColor="#D1D3D5"                            android:textSize="22sp" />                        <TextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_gravity="center_horizontal"                            android:layout_marginStart="20dp"                            android:layout_marginTop="15dp"                            android:layout_marginEnd="20dp"                            android:fontFamily="@font/lexend"                            android:text="Acess To Every Country "                            android:textAlignment="center"                            android:textColor="#D1D3D5"                            android:textSize="17sp" />                        <TextView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_gravity="center_horizontal"                            android:layout_marginStart="20dp"                            android:layout_marginTop="5dp"                            android:layout_marginEnd="20dp"                            android:fontFamily="@font/lexend"                            android:text="Cancel Anytime"                            android:textAlignment="center"                            android:textColor="#D1D3D5"                            android:textSize="17sp" />                        <ImageView                            android:layout_width="wrap_content"                            android:layout_height="wrap_content"                            android:layout_marginTop="10dp"                            android:src="@drawable/ic_new_line_server_bg" />                    </LinearLayout>                    <LinearLayout                        android:layout_width="match_parent"                        android:layout_height="match_parent"                        android:orientation="vertical">                        <androidx.constraintlayout.widget.ConstraintLayout                            android:layout_width="match_parent"                            android:layout_height="wrap_content"                            android:layout_centerInParent="true"                            android:layout_margin="15dp"                            android:background="@drawable/bg_lite"                            android:padding="@dimen/_10sdp"                            android:tint="@color/main">                            <ImageView                                android:id="@+id/premium_close_btn"                                android:layout_width="@dimen/_30sdp"                                android:layout_height="@dimen/_30sdp"                                android:clickable="true"                                android:padding="@dimen/_3sdp"                                android:src="@drawable/cross_icon"                                app:layout_constraintEnd_toEndOf="parent"                                app:layout_constraintTop_toTopOf="parent"                                app:tint="@color/colorgreen" />                            <LinearLayout                                android:id="@+id/verticallayout"                                android:layout_width="match_parent"                                android:layout_height="130dp"                                android:layout_marginTop="20dp"                                android:background="@drawable/linear_border_boxes"                                android:divider="@drawable/divider"                                android:dividerPadding="10dp"                                android:gravity="center"                                android:orientation="horizontal"                                android:showDividers="middle"                                app:layout_constraintEnd_toEndOf="parent"                                app:layout_constraintStart_toStartOf="parent"                                tools:ignore="MissingConstraints">                                <LinearLayout                                    android:id="@+id/premium_low"                                    android:layout_width="match_parent"                                    android:layout_height="match_parent"                                    android:layout_gravity="center"                                    android:layout_weight="1"                                    android:gravity="center"                                    android:orientation="vertical">                                    <TextView                                        android:layout_width="wrap_content"                                        android:layout_height="wrap_content"                                        android:layout_below="@+id/image"                                        android:fontFamily="@font/avalon"                                        android:text="1"                                        android:textColor="@color/white"                                        android:textSize="25sp"                                        android:textStyle="bold" />                                    <TextView                                        android:layout_width="wrap_content"                                        android:layout_height="wrap_content"                                        android:layout_below="@+id/image"                                        android:fontFamily="@font/avalon"                                        android:text="week"                                        android:textColor="@color/white"                                        android:textSize="11sp"                                        android:textStyle="bold" />                                    <TextView                                        android:id="@+id/third"                                        android:layout_width="match_parent"                                        android:textAlignment="center"                                        android:layout_height="wrap_content"                                        android:layout_below="@+id/image"                                        android:fontFamily="@font/avalon"                                        android:text="$2.99"                                        android:textColor="@color/white"                                        android:textSize="14sp"                                        android:textStyle="bold" />                                    <TextView                                        android:id="@+id/fourth"                                        android:layout_width="match_parent"                                        android:textAlignment="center"                                        android:layout_height="wrap_content"                                        android:layout_below="@+id/image"                                        android:layout_marginTop="@dimen/_10sdp"                                        android:fontFamily="@font/avalon"                                        android:text="$2.99/wk"                                        android:visibility="gone"                                        android:textColor="@color/white"                                        android:textSize="12sp"                                        android:textStyle="bold" />                                </LinearLayout>                                <LinearLayout                                    android:id="@+id/premium_mid"                                    android:layout_width="match_parent"                                    android:layout_height="match_parent"                                    android:layout_gravity="center"                                    android:layout_weight="1"                                    android:gravity="center"                                    android:orientation="vertical">                                    <TextView                                        android:layout_width="wrap_content"                                        android:layout_height="wrap_content"                                        android:fontFamily="@font/avalon"                                        android:text="1"                                        android:textColor="@color/white"                                        android:textSize="25sp"                                        android:textStyle="bold" />                                    <TextView                                        android:layout_width="wrap_content"                                        android:layout_height="wrap_content"                                        android:fontFamily="@font/avalon"                                        android:text="month"                                        android:textColor="@color/white"                                        android:textSize="11sp"                                        android:textStyle="bold" />                                    <TextView                                        android:id="@+id/thirdmonth"                                        android:layout_width="match_parent"                                        android:textAlignment="center"                                        android:layout_height="wrap_content"                                        android:fontFamily="@font/avalon"                                        android:text="$4.99"                                        android:textColor="@color/white"                                        android:textSize="14sp"                                        android:textStyle="bold" />                                    <TextView                                        android:id="@+id/fourthmonth"                                        android:layout_width="match_parent"                                        android:textAlignment="center"                                        android:layout_height="wrap_content"                                        android:layout_marginTop="@dimen/_10sdp"                                        android:fontFamily="@font/avalon"                                        android:text="$4.99/mo"                                       android:visibility="gone" android:textColor="@color/white"                                        android:textSize="12sp"                                        android:textStyle="bold" />                                </LinearLayout>                                <LinearLayout                                    android:id="@+id/premium_high"                                    android:layout_width="match_parent"                                    android:layout_height="match_parent"                                    android:layout_gravity="center"                                    android:layout_weight="1"                                    android:background="@drawable/shape_premium_right"                                    android:fontFamily="@font/avalon"                                    android:gravity="center"                                    android:orientation="vertical"                                    android:textStyle="bold">                                    <TextView                                        android:layout_width="wrap_content"                                        android:layout_height="wrap_content"                                        android:fontFamily="@font/avalon"                                        android:text="12"                                        android:textColor="@color/white"                                        android:textSize="25sp"                                        android:textStyle="bold" />                                    <TextView                                        android:layout_width="wrap_content"                                        android:layout_height="wrap_content"                                        android:layout_below="@+id/image"                                        android:fontFamily="@font/avalon"                                        android:text="month"                                        android:textColor="@color/white"                                        android:textSize="@dimen/_8sdp"                                        android:textStyle="bold" />                                    <TextView                                        android:id="@+id/twelveemonth"                                        android:layout_width="match_parent"                                        android:textAlignment="center"                                        android:layout_height="wrap_content"                                        android:fontFamily="@font/avalon"                                        android:text="$47.88"                                        android:textColor="@color/white"                                        android:textSize="14sp"                                        android:textStyle="bold" />                                    <TextView                                        android:layout_width="match_parent"                                        android:layout_height="wrap_content"                                        android:fontFamily="@font/avalon"                                        android:gravity="center_horizontal"                                        android:text="SAVE 58%"                                        android:textAlignment="center"                                        android:textColor="#fff"                                        android:textSize="@dimen/_8sdp"                                        android:textStyle="bold"                                        android:visibility="gone" />                                    <TextView                                        android:id="@+id/twelveeemonth"                                        android:layout_width="match_parent"                                        android:textAlignment="center"                                        android:layout_height="wrap_content"                                        android:layout_marginTop="@dimen/_5sdp"                                        android:fontFamily="@font/avalon"                                        android:gravity="center_horizontal"                                       android:visibility="gone"                                        android:text="$3.99/mo"                                        android:textColor="#fff"                                        android:textSize="12sp"                                        android:textStyle="bold" />                                </LinearLayout>                            </LinearLayout>                            <TextView                                android:id="@+id/no_commit"                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_marginTop="@dimen/_15sdp"                                android:fontFamily="@font/avalon"                                android:text="Cancel Anytime on Google Play Store"                                android:textColor="@color/text_color"                                android:textSize="@dimen/_9sdp"                                android:textStyle="bold"                                app:layout_constraintEnd_toEndOf="parent"                                app:layout_constraintStart_toStartOf="parent"                                app:layout_constraintTop_toBottomOf="@id/verticallayout" />                            <Button                                android:id="@+id/premium"                                android:layout_width="match_parent"                                android:layout_height="60dp"                                android:layout_marginHorizontal="20dp"                                android:layout_marginTop="20dp"                                android:background="@drawable/shape_rounded_sky_blue_button"                                android:fontFamily="@font/avalon"                                android:text="Get Premium"                                android:textAllCaps="false"                                android:textColor="#fff"                                android:textStyle="bold"                                app:layout_constraintEnd_toEndOf="parent"                                app:layout_constraintStart_toStartOf="parent"                                app:layout_constraintTop_toBottomOf="@id/no_commit" />                            <Button                                android:id="@+id/freetrial"                                android:layout_width="match_parent"                                android:layout_height="@dimen/_40sdp"                                android:layout_marginTop="20dp"                                android:background="@drawable/shape_rounded_sky_blue_button2"                                android:fontFamily="@font/avalon"                                android:text="Start 3 Days Free Trial"                                android:textAllCaps="false"                                android:textColor="@color/text_color"                                android:textStyle="bold"                                android:visibility="gone"                                app:layout_constraintEnd_toEndOf="parent"                                app:layout_constraintStart_toStartOf="parent"                                app:layout_constraintTop_toBottomOf="@id/premium" />                            <TextView                                android:id="@+id/restore"                                android:layout_width="wrap_content"                                android:layout_height="wrap_content"                                android:layout_marginTop="10dp"                                android:fontFamily="@font/avalon"                                android:text="Restore Purchase"                                android:textColor="@color/text_color"                                android:textSize="@dimen/_9sdp"                                android:textStyle="bold"                                android:visibility="gone"                                app:layout_constraintEnd_toEndOf="parent"                                app:layout_constraintStart_toStartOf="parent"                                app:layout_constraintTop_toBottomOf="@id/premium" />                            <LinearLayout                                android:layout_width="fill_parent"                                android:layout_height="match_parent"                                android:gravity="start"                                android:orientation="vertical"                                android:paddingStart="@dimen/_15sdp"                                android:paddingEnd="@dimen/_15sdp"                                app:layout_constraintEnd_toEndOf="parent"                                app:layout_constraintStart_toStartOf="parent"                                app:layout_constraintTop_toBottomOf="@+id/restore">                                <TextView                                    android:layout_width="wrap_content"                                    android:layout_height="wrap_content"                                    android:layout_gravity="center_horizontal"                                    android:layout_marginTop="@dimen/_5sdp"                                    android:layout_marginBottom="@dimen/_5sdp"                                    android:fontFamily="@font/lexend"                                    android:text="Subscription Terms"                                    android:textColor="@color/text_color"                                    android:textSize="@dimen/_10sdp"                                    android:textStyle="bold" />                                <TextView                                    android:layout_width="match_parent"                                    android:layout_height="wrap_content"                                    android:layout_marginTop="@dimen/_5sdp"                                    android:fontFamily="@font/avalon"                                    android:text="1. Subscription will be charged to your credit card through your Google account at confirmed of purchase."                                    android:textColor="@color/text_color"                                    android:textSize="14sp" />                                <TextView                                    android:layout_width="match_parent"                                    android:layout_height="wrap_content"                                    android:layout_marginTop="3dp"                                    android:fontFamily="@font/avalon"                                    android:text="2. Refunds are not avaiable for unused portions of a subscription. "                                    android:textColor="@color/text_color"                                    android:textSize="14sp" />                            </LinearLayout>                        </androidx.constraintlayout.widget.ConstraintLayout>                    </LinearLayout>                </LinearLayout>            </RelativeLayout>        </RelativeLayout>    </ScrollView></RelativeLayout>